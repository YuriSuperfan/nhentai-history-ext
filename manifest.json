{
  "manifest_version": 3,
  "name": "nhentai Reading History",
  "version": "1.0",
  "description": "Keeps track of doujins you've started reading on nhentai.net.",
  "permissions": ["storage"],
  "host_permissions": ["https://nhentai.net/*"],
  "action": {},
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://nhentai.net/g/*/*"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["style.css"],
      "matches": ["<all_urls>"]
    }
  ],
"content_security_policy": {
  "extension_pages": "default-src 'self'; img-src 'self' https://*.nhentai.net data:;"
},
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
